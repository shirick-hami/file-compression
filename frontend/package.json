    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "typescript": "^5.3.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shirick-hami/file-compression.git"
  },
  "keywords": [
    "huffman",
    "compression",
    "stencil",
    "web-components",
    "typescript"
  ]
}
