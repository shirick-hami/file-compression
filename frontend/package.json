{
  "name": "file-compression",
  "version": "1.0.0",
  "description": "Stencil.js frontend for Huffman Compressor with TypeScript",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/file-compression/file-compression.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "build:prod": "NODE_ENV=production stencil build",
    "build:prod:win": "cross-env NODE_ENV=production stencil build",
